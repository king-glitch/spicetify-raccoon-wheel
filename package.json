{
  "name": "raccoon-wheel",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "spicetify-creator --out=dist --minify",
    "watch": "spicetify-creator --watch",
    "apply": "spicetify config extensions raccoon-wheel.js && spicetify apply",
    "push": "cp -r dist/raccoon-wheel.js ~/.config/spicetify/Extensions/",
    "build-and-push": "bun run build && bun run push && bun run apply"
  },
  "license": "MIT",
  "devDependencies": {
    "spicetify-creator": "^1.0.17"
  },
  "dependencies": {
    "spcr-settings": "^1.3.1"
  }
}